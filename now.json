{
    "version": 2,
    "github": {
		"enabled": false
	},
    "builds": [
        { "src": "**/*", "use": "@now/static" }
    ],
    "routes": [
        { "src": "/.*", "headers": { "Link": "<https://cdn.jsdelivr.net>; rel=preconnect" }, "continue": true },
        { "handle": "filesystem" },
        { "src": "/.*", "status": 404, "dest": "404.html" }
    ]
}
